{
  "family": "myapp-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::480278898183:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "myapp",
      "image": "PLACEHOLDER",  // will be replaced by GitHub Actions
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
